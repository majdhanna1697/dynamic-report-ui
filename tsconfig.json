{
	"compilerOptions": {
		"target": "ES2015",
		"downlevelIteration": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"baseUrl": ".",
		"paths": {
			"*": ["*"]
		},
		"jsx": "react-jsx",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	}
}
